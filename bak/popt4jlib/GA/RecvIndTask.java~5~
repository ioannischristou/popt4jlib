package popt4jlib.GA;

import parallel.*;
import java.util.*;

public class RecvIndTask implements TaskObject {
  Vector _immpool;
  Vector _imms;
  boolean _isDone=false;

  public RecvIndTask(Vector immpool, Vector imms) {
    _immpool = immpool;
    _imms = imms;
  }

  public void run() {
    _immpool.addAll(_imms);
    _isDone = true;
  }

  public boolean isDone() { return _isDone; }

}
